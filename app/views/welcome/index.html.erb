<!-- header -->
<section id="header-bg">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <%= render '/hero' %>
        <br><br><br><br><br><br><br>
      </div>
    </div>
  </div>
</section>
<!-- end header -->

<!-- user view -->
<% if @user %>
  
<section id="user-feed">
  <div class="container, bottom7">
    <div class="row">
      <div class="col-md-4 text-center">
        <%= render '/repos' %>
      </div>
  
      <div class="col-md-4 text-center">
        <%= render '/user_feed' %><br>
      </div>
  
      <div class="col-md-4 text-center">
        <%= render '/organizations' %>
      </div>
    </div>
  </div>
</section>

  
<section id="contributions">
  <div class="container, bottom7">
    <div class="row">
      <div class="col-lg-12 text-center">
        <%= render '/contrib_activity' %>
      </div>
    </div>
  </div>
</section>
  

<% end %>
<!-- end user view -->
